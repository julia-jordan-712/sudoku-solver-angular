{
  "name": "sudoku-solver-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "cy-headless": "npx cypress run --component --browser chrome",
    "cy-headed": "npx cypress run --component --browser chrome --headed --no-exit --spec",
    "cy-open": "npx cypress open --component --browser chrome",
    "build": "ng build",
    "format": "npm run prettier && ng lint --fix",
    "generate-store": "ng generate @ngrx/schematics:store",
    "generate-feature-store": "ng generate @ngrx/schematics:feature --api false",
    "generate-action": "ng generate @ngrx/schematics:action",
    "generate-reducer": "ng generate @ngrx/schematics:reducer",
    "generate-selector": "ng generate @ngrx/schematics:selector",
    "generate-effect": "ng generate @ngrx/schematics:effect",
    "lint": "ng lint",
    "prettier": "prettier --write  \"./**/*\"",
    "start": "ng serve",
    "test": "ng test",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/material": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@ngrx/component-store": "^18.0.2",
    "@ngrx/effects": "^18.0.2",
    "@ngrx/eslint-plugin": "^18.0.2",
    "@ngrx/operators": "^18.0.2",
    "@ngrx/schematics": "^18.0.2",
    "@ngrx/store": "^18.0.2",
    "@ngrx/store-devtools": "^18.0.2",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.3.0",
    "uuid": "^9.0.1",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.0",
    "@angular-eslint/builder": "18.3.0",
    "@angular-eslint/eslint-plugin": "18.3.0",
    "@angular-eslint/eslint-plugin-template": "18.3.0",
    "@angular-eslint/schematics": "18.3.0",
    "@angular-eslint/template-parser": "18.3.0",
    "@angular/cli": "^18.2.0",
    "@angular/compiler-cli": "^18.2.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cypress": "^13.15.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-cypress": "^3.5.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ngx-translate-testing": "7.0.0",
    "prettier": "3.3.3",
    "typescript": "~5.4.5"
  }
}
